// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Items {
  id              Int      @id @default(autoincrement())
  name            String
  price           Float
  quantity        Int
  image           String
  category        String
  subCategory     String
  brand           String
  tag             String
  description     String
  sellerId        Int
}

